        <property name="eclipselink.jdbc.url" value="jdbc:mysql://localhost:3306/inmovinescrm"/>
        <property name="eclipselink.jdbc.driver" value="com.mysql.jdbc.Driver"/>
    </properties>
  </persistence-unit>
</persistence>
